{
  "name": "scbi-vscode",
  "displayName": "scbi-vscode",
  "description": "SCBI integration for VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.109.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TurboGit/scbi-vscode.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:scbi.build"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "scbi.build",
        "title": "SCBI: Build Target"
      }
    ],
    "problemMatchers": [
      {
        "name": "scbi-gcc",
        "base": "$gcc"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
